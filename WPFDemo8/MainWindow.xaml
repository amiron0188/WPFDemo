<reactiveui:ReactiveWindow x:Class="WPFDemo8.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFDemo8"
        xmlns:vms="clr-namespace:WPFDemo8"
        x:TypeArguments="vms:MainViewModel"
        xmlns:reactiveui="http://reactiveui.net"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBox x:Name="Name" Margin="10"/>
            <TextBox x:Name="Title" Margin="10"/>
            <Button x:Name="Show" Content="Show"/>
        </StackPanel>
    </Grid>
</reactiveui:ReactiveWindow>
